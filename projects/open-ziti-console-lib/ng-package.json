{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/open-ziti-console-lib",
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "assets": [
    {
      "input": "src/lib/assets",
      "glob": "**",
      "output": "assets"
    },
    {
      "input": "src/lib/html",
      "glob": "**",
      "output": "html"
    },
    {
      "input": "src/lib/shared-assets",
      "glob": "**",
      "output": "assets"
    }
  ]
}
