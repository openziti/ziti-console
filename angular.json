{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "openZiti": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-open-ziti",
      "sourceRoot": "projects/app-open-ziti/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-open-ziti",
            "index": "projects/app-open-ziti/src/index.html",
            "main": "projects/app-open-ziti/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-open-ziti/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-open-ziti/src/favicon.ico",
              "projects/app-open-ziti/src/assets",
              {
                "glob": "**/*",
                "input": "./projects/consoleLib/src/lib/assets/",
                "output": "/assets"
              }

            ],
            "styles": [
              "projects/app-open-ziti/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "openZiti:build:production"
            },
            "development": {
              "browserTarget": "openZiti:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "openZiti:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-open-ziti/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-open-ziti/src/favicon.ico",
              "projects/app-open-ziti/src/assets"
            ],
            "styles": [
              "projects/app-open-ziti/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "zacs": {
      "projectType": "library",
      "root": "projects/zacs",
      "sourceRoot": "projects/zacs/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/zacs/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/zacs/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/zacs/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/zacs/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ziti-console": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ziti-console",
      "sourceRoot": "projects/ziti-console/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ziti-console",
            "index": "projects/ziti-console/src/index.html",
            "main": "projects/ziti-console/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ziti-console/tsconfig.app.json",
            "assets": [
              "projects/ziti-console/src/favicon.ico",
              "projects/ziti-console/src/assets"
            ],
            "styles": [
              "projects/ziti-console/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ziti-console:build:production"
            },
            "development": {
              "browserTarget": "ziti-console:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ziti-console:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/ziti-console/tsconfig.spec.json",
            "assets": [
              "projects/ziti-console/src/favicon.ico",
              "projects/ziti-console/src/assets"
            ],
            "styles": [
              "projects/ziti-console/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "consoleLib": {
      "projectType": "library",
      "root": "projects/consoleLib",
      "sourceRoot": "projects/consoleLib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/consoleLib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/consoleLib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/consoleLib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/consoleLib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
